# Commands for CSS Grid

# GRID

(grid | greed | great) display:
  "display: grid;"
  key(enter)

(grid | greed | great) template (rows | rose):
  "grid-template-rows: ;"
  key(left)

(grid | greed | great) template columns:
  "grid-template-columns: ;"
  key(left)

(grid | greed | great) row gap:
  "grid-row-gap: ;"
  key(left)  

(grid | greed | great) column gap:
  "grid-column-gap: ;"
  key(left)  